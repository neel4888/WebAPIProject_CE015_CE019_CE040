@model IEnumerable<OnlineAdmissionSystem.Models.Student>

@{
    ViewBag.Title = "Merit";
}

<h2>Display</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.merit)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.merit)
        </td>
    </tr>
}
</table>